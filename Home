            jQuery(document).ready(function($){
                var page = 1;
                function loadMore() {
                    var data = {
                        'action': 'wp_more_posts_load',
                        'paged': page++
                    };
                    button.text('Loading...');
                    jQuery.post(wp_isotope_ajax.url, data, function(response) {
                        button.text('Load More Articles');
                        $('.cat-box-content > ul', '#wp-more-posts').append(response);
                    });
                }
                var button = $('button.load-more');

                button.click(function(e){
                    loadMore();
                }).css({'display':'block'});
                jQuery(document).ajaxError(function (a,b,c,d) {
                    if (d=='abort') {
                        return;
                    }
                    button.text('Load More Articles');
                });

            });
        </script>
